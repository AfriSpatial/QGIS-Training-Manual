msgstr ""

#: ../../rasters/terrain_analysis.rst:160
# 9f4fee5c11e44a2b8b45f2dceb2700a4
msgid ":kbd:`aspect@1 <= 90 OR aspect@1 >= 270`"
msgstr ""

